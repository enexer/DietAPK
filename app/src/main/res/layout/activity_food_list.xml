<?xml version="1.0" encoding="utf-8"?>
<android.widget.RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.as.dieta.FoodListActivity">

    <TextView
        android:id="@+id/textView10"
        android:layout_width="365dp"
        android:layout_height="30dp"
        android:text="  NAZWA"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="2dp"
        android:layout_below="@+id/buttonAddProduct"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/textViewFoodListDM"
        android:layout_alignEnd="@+id/textViewFoodListDM"
        android:layout_marginRight="236dp"
        android:layout_marginEnd="236dp" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="365dp"
        android:layout_height="30dp"
        android:layout_alignBaseline="@+id/textView10"
        android:layout_alignBottom="@+id/textView10"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_marginLeft="58dp"
        android:layout_marginStart="58dp"
        android:layout_toEndOf="@+id/buttonAddProduct"
        android:text="WAGA | KCAL  |  W  |  B  |  T  "
        android:textAlignment="textStart"
        tools:layout_editor_absoluteX="6dp"
        tools:layout_editor_absoluteY="2dp"
        android:layout_toRightOf="@+id/buttonAddProduct" />

    <TextView
        android:id="@+id/textViewFoodListDM"
        android:layout_width="378dp"
        android:layout_height="23dp"
        android:text="TextView"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="2dp" />

    <Button
        android:id="@+id/buttonAddProduct"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textViewFoodListDM"
        android:text="DODAJ" />

    <ListView
        android:id="@+id/listViewSelected"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView10"
        android:layout_alignParentBottom="true"
        android:layout_marginBottom="195dp" />
    <TextView
        android:id="@+id/textViewFLSum2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_above="@+id/textViewFLSum"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/textViewFLSum"
        android:layout_alignEnd="@+id/textViewFLSum" />

    <TextView
        android:id="@+id/textViewFLSum"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_above="@+id/textViewFLTarget"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/textViewFoodListDM"
        android:layout_alignEnd="@+id/textViewFoodListDM" />

    <TextView
        android:id="@+id/textViewFLTarget"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginBottom="130dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignRight="@+id/textViewFLSum2"
        android:layout_alignEnd="@+id/textViewFLSum2" />

    <ProgressBar
        android:id="@+id/progressBarFL"
        style="?android:attr/progressBarStyleHorizontal"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginBottom="94dp"
        android:layout_marginTop="32dp"
        android:layout_alignParentBottom="true"
        android:layout_alignTop="@+id/textViewFLTarget" />
</android.widget.RelativeLayout>
