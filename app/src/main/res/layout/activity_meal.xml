        tools:layout_editor_absoluteX="56dp"
        tools:layout_editor_absoluteY="244dp" />

    <Button
        android:id="@+id/buttonMeal1"
        android:layout_width="286dp"
        android:layout_height="49dp"
        android:layout_above="@+id/buttonMeal2"
        android:layout_alignLeft="@+id/buttonMeal2"
        android:layout_alignStart="@+id/buttonMeal2"
        android:background="@android:color/holo_blue_bright"
        android:text="Śniadanie"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="155dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Kalorie : 0"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="16dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Białko: 0"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="33dp"
        android:layout_below="@+id/textView7"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Węglowodany: 0"
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="50dp"
        android:layout_below="@+id/textView8"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Tłuszcze: 0 "
        tools:layout_editor_absoluteX="16dp"
        tools:layout_editor_absoluteY="67dp"
        android:layout_below="@+id/textView3"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />
</android.widget.RelativeLayout>
