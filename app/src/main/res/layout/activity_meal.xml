        android:layout_alignLeft="@+id/buttonMeal5"
        android:layout_alignStart="@+id/buttonMeal5" />

    <Button
        android:id="@+id/buttonMeal5"
        android:layout_width="285dp"
        android:layout_height="48dp"
        android:background="@android:color/holo_blue_bright"
        android:text="Kolacja"
        tools:layout_editor_absoluteX="55dp"
        tools:layout_editor_absoluteY="339dp"
        android:layout_below="@+id/buttonMeal4"
        android:layout_centerHorizontal="true" />

    <Button
        android:id="@+id/buttonMeal3"
        android:layout_width="285dp"
        android:layout_height="42dp"
        android:layout_alignEnd="@+id/buttonMeal4"
        android:layout_alignLeft="@+id/buttonMeal4"
        android:layout_alignRight="@+id/buttonMeal4"
        android:layout_alignStart="@+id/buttonMeal4"
        android:layout_below="@+id/buttonMeal2"
