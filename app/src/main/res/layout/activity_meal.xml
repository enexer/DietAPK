        tools:layout_editor_absoluteY="286dp"
        android:layout_marginTop="42dp"
        android:layout_below="@+id/buttonMeal2"
        android:layout_alignRight="@+id/buttonMeal2"
        android:layout_alignEnd="@+id/buttonMeal2" />

    <Button
        android:id="@+id/buttonMeal2"
        android:layout_width="286dp"
        android:layout_height="41dp"
